{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "es6": true,
        "worker": true
    },
    "parserOptions": {
        "ecmaVersion": 10,
        "sourceType": "module"
    },
    "rules": {
        "no-mixed-operators": "off",
        "no-param-reassign": "off"
    }
}
